{% extends 'CrudforgeBundle::admin.html.twig' %}
{% block body %}
<h2>Users creation</h2>

<form action="{{ path('users_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('users') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}